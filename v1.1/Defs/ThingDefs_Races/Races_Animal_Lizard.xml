        <eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
        <eggCountRange>
          <min>1</min>
          <max>2</max>
        </eggCountRange>
      </li>
    </comps>
    <race>
      <baseHungerRate>0.35</baseHungerRate>
      <body>QuadrupedAnimalWithClawsTailAndJowl</body>
      <petness>1</petness>
      <herdAnimal>true</herdAnimal>
      <baseBodySize>1.3</baseBodySize>
      <baseHealthScale>1.2</baseHealthScale>
      <foodType>OmnivoreRoughAnimal</foodType>
      <leatherDef>Leather_ACP</leatherDef>
      <useMeatFrom>Iguana</useMeatFrom>
      <nameOnTameChance>1</nameOnTameChance>
      <trainability>Advanced</trainability>
      <wildness>0.45</wildness>
      <nuzzleMtbHours>12</nuzzleMtbHours>
      <packAnimal>true</packAnimal>
      <gestationPeriodDays>25</gestationPeriodDays>
      <litterSizeCurve>
        <points>
          <li>(0.5, 0)</li>
          <li>(1, 1)</li>
          <li>(3, 1)</li>
          <li>(3.5, 0)</li>
        </points>
      </litterSizeCurve>
      <lifeExpectancy>55</lifeExpectancy>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.09</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.35</minAge>
          <soundWounded>Pawn_Iguana_Wounded</soundWounded>
          <soundDeath>Pawn_Iguana_Death</soundDeath>
          <soundCall>Pawn_Iguana_Call</soundCall>
          <soundAngry>Pawn_Iguana_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
    </race>
    <tradeTags>
